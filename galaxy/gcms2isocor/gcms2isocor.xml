<tool id="gcms2isocor" name="Conversion GCMS PostRun Analysis to IsoCor" version="0.1.0" python_template_version="3.5">
    <requirements>
    </requirements>
    <command detect_errors="exit_code"><![CDATA[
        java -jar '$__tool_directory__/P2M2WorkflowsTools-assembly-0.1.jar' '$input1' '$input2' --out '$output1'
    ]]></command>
    <inputs>
        <param type="data" name="input1" format="txt" />
        <param type="data" name="input2" format="txt" />
    </inputs>
    <outputs>
        <data name="output1" format="tsv" />
    </outputs>
    <tests>
        <test>
            <param name="input1" value="13CPROT1.txt"/>
            <param name="input2" value="13CPROT2.txt"/>
            <output name="output1" file="input_isocor.tsv"/>
        </test>
    </tests>
